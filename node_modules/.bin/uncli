#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node" --experimental-modules "$basedir/../un-cli/un.mjs" "$@"
  ret=$?
else 
  node --experimental-modules "$basedir/../un-cli/un.mjs" "$@"
  ret=$?
fi
exit $ret
